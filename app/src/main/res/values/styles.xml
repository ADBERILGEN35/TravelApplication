        <item name="fontFamily">@font/source_sans_pro</item>
        <item name="tabGravity">start</item>
        <item name="tabIndicatorColor">@color/tabs_selected</item>
        <item name="tabSelectedTextColor">@color/tabs_selected</item>
        <item name="tabTextColor">@color/tabs_not_selected</item>
        <item name="textStartPadding"></item>
        <item name="tabIndicatorFullWidth">false</item>
        <item name="tabMode">scrollable</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="Theme.App" parent="Theme.MaterialComponents">
