        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/lineer_backround</item>
        <item name="android:selectableItemBackgroundBorderless">@color/black</item>
        <item name="selectableItemBackgroundBorderless">@color/black</item>
